                                0x2D 0x08
                                0x30 0x09
                                0x31 0x0a
                                0x32 0x20
                                0x33 0x60
                                0x34 0x0C
                                0x35 0x15
                                0x36 0x1B
                                0x37 0x3c
                                0x40 0x00
                                0x41 0x00
                                0x42 0x00
                                0x43 0x00
                                0x44 0x00
                                0x45 0x05
                                0x46 0x00
                                0x47 0x00
                                0x48 0x00
                                0x49 0x00
                                0x4A 0x00
                                0x4B 0x00
                                0x4C 0x00
                                0x4D 0x00
                                0x4E 0x80
                                0x4F 0x0C
